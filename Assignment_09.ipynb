      "source": [
        "n = int(input(\"Enter the limit number : \"))\n",
        "prime_list = []\n",
        "for k in range(2,n) :\n",
        "  prime = 0\n",
        "  for i in range(1,n+1) :\n",
        "    if k % i == 0 :\n",
        "      prime += 1\n",
        "  if prime <= 2 :\n",
        "    prime_list.append(k)\n",
        "\n",
        "print(prime_list)\n",
        "  \n",
        "\n",
        "\n"
      ],
