        "sentence = str(input(\"Enter the sentence that will count :\")).lower()\n",
        "letters = list(set(sentence))\n",
        "result = {}\n",
        "\n",
        "for i in range(0,len(letters)) :\n",
        "  result[letters[i]] = sentence.count(letters[i])\n",
        "\n",
        "print(result)"
      ],
